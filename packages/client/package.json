{
  "name": "snipsnap-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:docker": "docker run -p 80:80 --detach --name snipsnap-client snipsnap/client:0.1",
    "build": "tsc -b && vite build",
    "build:docker": "docker build ./ --tag snipsnap/client:0.1",
    "lint": "cd ../.. && npm run lint -- packages/client",
    "lint:fix": "cd ../.. && npm run lint:fix -- packages/client",
    "format": "cd ../.. && npm run format -- packages/client",
    "preview": "vite preview",
    "generate": "graphql-codegen --config ./graphql/config/server.yml"
  },
  "dependencies": {
    "@apollo/client": "^4.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/vite": "^4.1.12",
    "graphql": "^16.11.0",
    "jwt-decode": "^4.0.0",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.8.2",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "validator": "^13.15.15",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/named-operations-object": "^3.1.1",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@parcel/watcher": "^2.5.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/validator": "^13.15.2",
    "@vitejs/plugin-react": "^5.0.1",
    "daisyui": "^5.0.50",
    "typescript": "~5.8.3",
    "vite": "^7.1.3"
  }
}
